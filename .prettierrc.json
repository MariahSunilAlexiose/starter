{
  "trailingComma": "es5",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "",
    "^(next/(.*)$)|^(next$)",
    "",
    "^(@context|@cmp|@|@img)(/.*)$",
    "",
    "^\\.\\./.*$",
    "",
    "^[.]",
    "",
    ".*\\.css$"
  ]
}
