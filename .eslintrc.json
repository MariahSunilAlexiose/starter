{
  "extends": [
    "next/core-web-vitals",
    "plugin:react/jsx-runtime",
    "next",
    "prettier"
  ],
  "ignorePatterns": [
    "*.css",
    "*.json",
    ".md",
    ".husky/*",
    ".next/*",
    ".vscode/*",
    "node_modules/*",
    "src/fonts/*",
    "public/*",
    "next-env.d.ts",
    "next.config.ts",
    "yarn.lock",
    ".env*.local",
    ".gitignore",
    ".prettierignore",
    "next.config.mjs",
    "postcss.config.mjs",
    "README.md",
    "tailwind.config.ts",
    "tsconfig.tsbuildinfo"
  ],
  "rules": {
    "valid-typeof": "error",
    "use-isnan": "error",
    "no-unused-vars": "error",
    "no-import-assign": "error",
    "no-duplicate-imports": "error",
    "no-duplicate-case": "error",
    "no-dupe-keys": "error",
    "no-dupe-else-if": "error",
    "no-dupe-args": "error",
    "no-cond-assign": "error",
    "no-compare-neg-zero": "error",
    "no-func-assign": "error"
  }
}
